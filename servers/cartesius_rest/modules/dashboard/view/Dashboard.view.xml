<mvc:View
	height="100%"
	controllerName="sap.ui.Cartesius.modules.dashboard.controller.Dashboard"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	>
	<Page
		showHeader="false"
		enableScrolling="false" >


		<TileContainer
			id="container"
			tileDelete="handleTileDelete"
			tiles="{
				path: '/TileCollection'
			}">
			<StandardTile
				icon="sap-icon://{icon}"
				type="{type}"
				number="{number}"
				numberUnit="{numberUnit}"
				title="{title}"
				info="{info}"
				infoState="{infoState}"
				id="tilez"
				press="handlePress"
				app:key="{object}"/>
		</TileContainer>
		
	</Page>
</mvc:View>
